import sys, os, time
sys.path.append("/home/thomas/Astro/Python/Orbs")

import orbs
from orbs.orbs import Orbs
from orbs.core import Tools

to = Tools()

f = to.open_file('liste', 'r')

for line in f:
    line = line.strip()
    file_name = str(line)
    line = line.split('.')
    if len(line[0]) < 8:
        print '%s\t\tMISC\t\tSTD_MISC/%s'%(line[0], file_name)
    else:
        print '%s\tMISC\t\tSTD_MISC/%s'%(line[0], file_name)

    
